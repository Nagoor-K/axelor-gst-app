<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">
	
	<grid model="com.axelor.invoice.db.InvoiceLine" title="Invoices" name="invoice-grid" onNew="">
			<field name="product" />
			<field name="hsbn"/>
			<field name="item"/>
			<field name="qty"/>
			<field name="price"/>
			<field name="netamt"/>
			<field name="gstrate"/>
			<field name="igst"/>
			<field name="sgst"/>
			<field name="cgst"/>
			<field name="grossamt"/>
			
	</grid>
	
	<form model="com.axelor.invoice.db.InvoiceLine" title="Invoice" name="invoice-form">
			<panel>
				<field name="product" />
				<field name="hsbn"/>
				<field name="item"/>
				<field name="qty"/>
				<field name="price"/>
				<field name="netamt"/>
				<field name="gstrate"/>
				<field name="igst"/>
				<field name="sgst"/>
				<field name="cgst"/>
				<field name="grossamt"/>
			</panel>
	</form>

</object-views>

