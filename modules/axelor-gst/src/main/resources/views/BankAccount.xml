<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">
	
	<grid model="com.axelor.company.db.BankAccount" title="Bank Account" name="bank-grid" onNew="action.on.create.new">
			<field name="bank"/>
			<field name="accountno"/>
			<field name="ifsc" />
			<field name="type" />
			<field name="address" />
	</grid>
	
	<form model="com.axelor.company.db.BankAccount" title="Bank Account" name="bank-form">
			<panel>
				<field name="bank"/>
				<field name="accountno"/>
				<field name="ifsc" />
				<field name="type"/>
				<field name="address" />
			</panel>
	</form>

</object-views>
