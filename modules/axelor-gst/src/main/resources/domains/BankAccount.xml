<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

<module package="com.axelor.company.db" name="BankAccount"/>

<entity name="BankAccount" table="bank_account_table">

<string name="bank" title="Bank" required="true" />
<string name="accountno" title="Account No" required="true"/>
<string name="ifsc" title="IFSC" required="true"/>
<string name="type" selection="bankaccount.select.account.type" default="current"/>
<string name="address" title="Address" large="true"/>
	
</entity>


</domain-models>